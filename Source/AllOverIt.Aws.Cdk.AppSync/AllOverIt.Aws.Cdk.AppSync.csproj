    <ItemGroup>
        <PackageReference Include="Amazon.CDK.AWS.AppSync" Version="1.121.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\AllOverIt\AllOverIt.csproj" />
    </ItemGroup>

</Project>
