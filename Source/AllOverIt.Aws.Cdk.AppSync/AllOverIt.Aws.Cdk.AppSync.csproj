<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <LangVersion>latest</LangVersion>
        <Version>0.13.1</Version>
        <Authors>Malcolm Smith</Authors>
        <Company></Company>
        <Copyright>Copyright Â© 2021</Copyright>
        <Description>A library to help build AWS Graphql schemas using a code-first approach.</Description>
        <PackageTags>utilities</PackageTags>
