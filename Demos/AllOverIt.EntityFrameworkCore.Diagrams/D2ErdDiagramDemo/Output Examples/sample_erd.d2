  source-arrowhead: {
    shape: cf-one-required
  }
  target-arrowhead: {
    shape: cf-many
  }
  style: {
    font-size: 18
    font-color: blue
    bold: true
  }
}

Blog.Id <-> Post.BlogId: {
  source-arrowhead: {
